{
  "name": "cl-nestjs-alicloud-kms",
  "version": "0.0.1",
  "description": "用于集成阿里云 KMS（密钥管理服务）以获取密钥和配置的 NestJS 模块",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChasLui/cl-nestjs-alicloud-kms.git"
  },
  "homepage": "https://github.com/ChasLui/cl-nestjs-alicloud-kms",
  "license": "MIT",
  "scripts": {
    "playground": "tsx playground/main.ts",
    "playground:build": "rimraf dist-playground && unbuild --config playground.config.ts && chmod +x dist-playground/index.mjs",
    "playground:run": "node dist-playground/index.mjs",
    "dev": "tsx playground/main.ts",
    "build": "rimraf dist && unbuild",
    "build:dev": "unbuild --stub",
    "build:all": "pnpm build && pnpm playground:build",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:modules": "node test-modules.js",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "release-it",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*": "prettier -w -u"
  },
  "keywords": [
    "nestjs",
    "alicloud",
    "kms",
    "key-management",
    "secrets",
    "configuration",
    "typescript",
    "aliyun",
    "cloud",
    "security"
  ],
  "author": "ChasLui <chaslui@outlook.com> (https://twitter.com/chaslui1)",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.15.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@babel/types": "^7.25.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "release-it": "^17.6.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "bugs": {
    "url": "https://github.com/ChasLui/cl-nestjs-alicloud-kms/issues"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "type-fest": "^4.41.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@alicloud/kms20160120": "^2.3.1",
    "reflect-metadata": "^0.2.2"
  }
}
